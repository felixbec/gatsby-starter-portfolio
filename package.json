    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.3.0",
    "algoliasearch": "^3.35.1",
    "bootstrap": "^4.4.1",
    "font-awesome": "^4.7.0",
    "formik": "^2.1.2",
    "gatsby": "^2.18.17",
    "gatsby-plugin-emotion": "^4.1.18",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-layout": "^1.1.21",
